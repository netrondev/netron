{
  "productName": "netron",
  "version": "0.1.0",
  "identifier": "com.tauri.dev0",
  "build": {
    "beforeDevCommand": "cargo leptos watch",
    "beforeBuildCommand": "cargo leptos build",
    "devUrl": "http://localhost:8000",
    "frontendDist": "../target/site/pkg/"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "template",
        "width": 800,
        "height": 600
      }
    ],
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}